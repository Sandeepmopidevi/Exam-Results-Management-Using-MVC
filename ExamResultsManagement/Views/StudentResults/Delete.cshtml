@model ExamResultsManagement.Models.StudentResult

<h2>Delete Student Result</h2>

<h3>Are you sure you want to delete this result?</h3>
<div>
    <h4>StudentResult</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Student Name</dt>
        <dd class="col-sm-10">@Model.StudentName</dd>
        <dt class="col-sm-2">Subject 1</dt>
        <dd class="col-sm-10">@Model.Subject1</dd>
        <dt class="col-sm-2">Marks 1</dt>
        <dd class="col-sm-10">@Model.Marks1</dd>
        <dt class="col-sm-2">Subject 2</dt>
        <dd class="col-sm-10">@Model.Subject2</dd>
        <dt class="col-sm-2">Marks 2</dt>
        <dd class="col-sm-10">@Model.Marks2</dd>
        <dt class="col-sm-2">Subject 3</dt>
        <dd class="col-sm-10">@Model.Subject3</dd>
        <dt class="col-sm-2">Marks 3</dt>
        <dd class="col-sm-10">@Model.Marks3</dd>
        <dt class="col-sm-2">Subject 4</dt>
        <dd class="col-sm-10">@Model.Subject4</dd>
        <dt class="col-sm-2">Marks 4</dt>
        <dd class="col-sm-10">@Model.Marks4</dd>
        <dt class="col-sm-2">Subject 5</dt>
        <dd class="col-sm-10">@Model.Subject5</dd>
        <dt class="col-sm-2">Marks 5</dt>
        <dd class="col-sm-10">@Model.Marks5</dd>
        <dt class="col-sm-2">Exam Month</dt>
        <dd class="col-sm-10">@Model.ExamMonth</dd>
        <dt class="col-sm-2">Exam Year</dt>
        <dd class="col-sm-10">@Model.ExamYear</dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-secondary">Back to List</a>
    </form>
</div>
